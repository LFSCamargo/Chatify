{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "format": "prettier --write '{.,__tests__,src/**}/*.{js,jsx,ts,tsx}'",
    "lint": "tslint -p .",
    "precommit": "tsc && lint-staged",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "studio": "open -a /Applications/Android\\ Studio.app ./android/",
    "test": "jest",
    "tsc": "tsc -p .",
    "xcode": "open ios/$npm_package_name.xcodeproj",
    "update-schema": "cd data && apollo-codegen introspect-schema http://localhost:5000 --output schema.json",
    "postinstall": "yarn codegen",
    "codegen": "apollo codegen:generate --localSchemaFile=data/schema.json --target=typescript && rm -rf __generated__"
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "react": "16.7.0-alpha.2",
    "react-native": "npm:@brunolemos/react-native",
    "react-native-gesture-handler": "^1.0.12",
    "styled-components": "^3.4.9",
    "react-apollo": "^2.3.2",
    "apollo": "^2.1.8",
    "apollo-boost": "^0.1.22",
    "apollo-codegen": "^0.20.2",
    "apollo-link-context": "^1.0.10",
    "react-apollo-hooks": "^0.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@types/jest": "^23.3.11",
    "@types/react": "^16.7.18",
    "@types/react-native": "^0.57.25",
    "@types/react-test-renderer": "^16.0.3",
    "babel-jest": "23.6.0",
    "babel-preset-react-native": "^5.0.2",
    "husky": "^1.3.1",
    "jest": "23.6.0",
    "lint-staged": "^8.1.0",
    "metro-react-native-babel-preset": "0.51.1",
    "prettier": "^1.15.3",
    "react-native-typescript-transformer": "^1.2.11",
    "react-test-renderer": "16.6.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  }
}
