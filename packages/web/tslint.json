{
  "extends": [
    "tslint-react",
    "tslint-config-airbnb"
  ],
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "variable-name": false,
    "import-name": false,
    "align": false,
    "jsx-no-lambda": false,
    "max-line-length": [
      true,
      120
    ]
  }
}