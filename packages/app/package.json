{
  "name": "@chatify/app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "format": "prettier --write '{.,__tests__,src/**}/*.{js,jsx,ts,tsx}'",
    "start": "node ../../node_modules/react-native/local-cli/cli.js start --projectRoot ../../",
    "studio": "open -a /Applications/Android\\ Studio.app ./android/",
    "android": "react-native run-android",
    "test": "jest",
    "prettier:all": "prettier 'src/**/*.ts*' --write",
    "xcode": "open ios/$npm_package_name.xcodeproj",
    "ios": "react-native run-ios",
    "update-schema": "cd data && apollo-codegen introspect-schema http://localhost:5000 --output schema.json",
    "postinstall": "yarn codegen",
    "codegen": "apollo codegen:generate --localSchemaFile=data/schema.json --target=typescript && rm -rf __generated__",
    "codegen:watch": "apollo codegen:generate --localSchemaFile=data/schema.json --target=typescript --watch && rm -rf __generated__"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts/"
    ]
  },
  "dependencies": {
    "apollo": "^2.1.8",
    "apollo-boost": "^0.1.22",
    "apollo-client-preset": "^1.0.8",
    "apollo-codegen": "^0.20.2",
    "apollo-link-context": "^1.0.10",
    "apollo-link-ws": "^1.0.17",
    "debounce": "^1.2.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "gravatar": "^1.8.0",
    "idx": "^2.5.2",
    "moment": "^2.29.4",
    "ramda": "^0.26.1",
    "react": "16.8.0",
    "react-apollo": "^2.3.2",
    "react-apollo-hooks": "^0.2.1",
    "react-native": "0.59.4",
    "react-native-callkeep": "^2.0.2",
    "react-native-dotenv": "^0.2.0",
    "react-native-fetch-blob": "^0.10.8",
    "react-native-gesture-handler": "^1.0.12",
    "react-native-gifted-chat": "^0.5.0",
    "react-native-image-picker": "^0.27.1",
    "react-native-image-resizer": "^1.0.0",
    "react-native-in-app-notification": "^3.0.0",
    "react-native-keyboard-spacer": "^0.4.1",
    "react-native-snap-carousel": "^3.7.5",
    "react-native-webrtc": "1.67.1",
    "react-navigation": "^3.0.5",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "styled-components": "^3.4.9",
    "subscriptions-transport-ws": "^0.9.16",
    "tslint-plugin-graphql": "^0.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/runtime": "^7.4.2",
    "@types/debounce": "^1.2.0",
    "@types/gravatar": "^1.4.28",
    "@types/jest": "^23.3.10",
    "@types/moment": "^2.13.0",
    "@types/ramda": "^0.25.46",
    "@types/react": "^16.7.13",
    "@types/react-native": "^0.57.17",
    "@types/react-native-fetch-blob": "^0.10.4",
    "@types/react-native-keyboard-spacer": "^0.4.1",
    "@types/react-native-snap-carousel": "^3.7.1",
    "@types/react-navigation": "^3.0.7",
    "@types/react-redux": "^7.0.9",
    "@types/react-test-renderer": "^16.0.3",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.7",
    "@types/styled-components": "^3.0.1",
    "babel-jest": "23.6.0",
    "babel-preset-react-native": "^5.0.2",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.0",
    "metro-react-native-babel-preset": "^0.53.1",
    "prettier": "^1.15.3",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.8.3",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.1"
  }
}